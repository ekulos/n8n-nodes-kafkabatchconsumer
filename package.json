{
  "name": "n8n-nodes-kafka-batch-consumer",
  "version": "1.0.21",
  "description": "N8N node for consuming Kafka messages in batches",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "cp -r src/nodes/KafkaBatchConsumer/*.svg dist/nodes/KafkaBatchConsumer/",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "kafka",
    "batch",
    "consumer"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/KafkaBatchConsumer/KafkaBatchConsumer.node.js"
    ]
  },
  "author": "Luca Pisani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekulos/n8n-nodes-kafkabatchconsumer.git"
  },
  "bugs": {
    "url": "https://github.com/ekulos/n8n-nodes-kafkabatchconsumer/issues"
  },
  "homepage": "https://github.com/ekulos/n8n-nodes-kafkabatchconsumer#readme",
  "dependencies": {
    "kafkajs": "^2.2.4",
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
